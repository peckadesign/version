{
	"name": "pd/version",
	"description": "Filtr pro zjisštění verze obsahu URL",
	"extra": {
		"extensions": {
			"pd.version": "Pd\\Version\\Extension"
		}
	},
	"require": {
		"php": "7.1.* || 7.2.* || 7.3.* || 7.4.*",
		"nette/http": "~2.0 || ~3.0",
		"nette/caching": "2.4.* || 2.5.*",
		"nette/application": "2.3.* || 2.4.*",
		"kdyby/console": "2.6.* || 2.7.*",
		"symfony/console": "~2.3 || ~3.0 || ~4.0"
	},
	"require-dev": {
		"pd/coding-standard": "1.17.*",
		"nette/tester": "2.3.*",
		"nette/bootstrap": "2.4.*",
		"phpstan/phpstan-strict-rules": "0.12.*"
	},
	"suggest": {
		"nette/caching": "Caching support",
		"nette/di": "Register filter using extension"
	},
	"autoload": {
		"psr-4": {
			"Pd\\Version\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/"
		]
	}
}
